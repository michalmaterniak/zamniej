{% extends 'page/base.html.twig' %}
{% block content %}
    <div class="row">
        <div class="col-md-12">
            <h1>{{ page.subpage.seo.header }}</h1>
        </div>

        <div class="col-md-12">
            {{ page.subpage.content|raw }}
        </div>
        {% for article in page.subpage.articles %}
            <a href="{{ article.subpage.slug }}">{{ article.subpage.subpage.name }}</a><br><br>
            <img src="{{ article.subpage.headerMini.modifyPath }}">
        {% endfor %}
    </div>
{% endblock %}
{% block assets_end %}
    <script>
        $(document).ready(function () {
            $(document).on('click', '#some', function () {
                alert();
            });

        });
    </script>
{% endblock %}
